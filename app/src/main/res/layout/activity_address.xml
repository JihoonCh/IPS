            android:id="@+id/gangbuk"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Gangbuk-gu" />
    </TableRow>

    <TableRow>

