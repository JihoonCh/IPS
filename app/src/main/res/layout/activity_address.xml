<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="800dp"
    android:background="@color/orange1"
    >

    <TableRow>

        <TextView
            android:textColor="@color/gray1"
            android:id="@+id/textView2"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:layout_span="3"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:gravity="center"
            android:text="Select your Current Location"
            android:textSize="20sp" />

    </TableRow>
    <TableRow>

        <Button
            android:layout_margin="5dp"
            android:background="@color/pink"
            android:id="@+id/guro"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Guro-gu" />

        <Button
            android:layout_margin="5dp"
            android:background="@color/pink"
            android:id="@+id/jung"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Jung-gu" />

        <Button
            android:layout_margin="5dp"
            android:background="@color/pink"
            android:id="@+id/mapo"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Mapo-gu" />

    </TableRow>

    <TableRow>

        <Button
            android:layout_margin="5dp"
            android:id="@+id/jongno"
            android:background="@color/pink"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Jongno-gu" />

        <Button
            android:layout_margin="5dp"
            android:id="@+id/nowon"
            android:background="@color/pink"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Nowon-gu" />

    </TableRow>


    <TableRow>

        <Button
            android:layout_margin="5dp"
            android:id="@+id/gangnam"
            android:background="@color/pink"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Gangnam-gu" />

        <Button
            android:layout_margin="5dp"
            android:id="@+id/gangdong"
            android:background="@color/pink"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Gangdong-gu" />

    </TableRow>


    <TableRow>

        <Button
            android:layout_margin="5dp"
            android:background="@color/pink"
            android:fontFamily="@font/sun_b"
            android:layout_weight="1"
            android:id="@+id/songpa"
            android:text="Songpa-gu"
            android:layout_width="wrap_content"
            android:layout_height="40dp"/>

        <Button
            android:layout_margin="5dp"
            android:background="@color/pink"
            android:id="@+id/seocho"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:fontFamily="@font/sun_b"
            android:text="Seocho-gu" />

    </TableRow>
    <TableRow>
        <Button
            android:layout_margin="5dp"
