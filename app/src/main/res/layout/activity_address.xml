            android:fontFamily="@font/sun_b"
            android:layout_weight="1"
            android:id="@+id/gwangjin"
            android:text="Gwangjin-gu"
            android:layout_width="wrap_content"
            android:layout_height="40dp" />

        <Button
            android:layout_margin="5dp"
